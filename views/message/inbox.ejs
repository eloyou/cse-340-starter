<% if (title) {%>
<h1><%= title %></h1>
<% } %>

<%- messages() %>

<% if (errors) { %>
  <ul class="notice">
 <% errors.array().forEach(error => { %>
   <li><%= error.msg %></li>
<%  }) %>
 </ul>
<% } %>

<ul class="inbox">
    <li><a href="/message/new?user=<%= locals.accountData.account_id %>">Create new messages</a></li>
    <li><a id="archive-link" href="/message/archive?user=<%= locals.accountData.account_id %>"></a></li>
</ul>

<table id="inbox-list" class="message-table">

</table>



<script src="../../js/message.js"></script>